#include<iostream>

   using namespace std;

   int main(){

   int costo,dia,distancia;

   cout<<"ingrese la distancia"<<endl;
   cin>>distancia;

   cout<<"ingresa dias de estancia" <<endl;
   cin>>dia;

	if(((distancia*2)>800) && (dia>7))
	
		costo=distancia*2*0.17*.70;

	else
	
		costo=distancia*2*0.17;


		cout<<costo;



return 0;

}



